{
  "name": "WayTab-react",
  "description": "Change your new tab page to be more productive",
  "version": "2.0",
  "author": [
    "Andy Wang",
    "Andrew Ward"
  ],
  "content_security_policy": "script-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU='; object-src 'self'",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background/defaulttabs.json",
      "background/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "res/icon16.png",
      "32": "res/icon32.png",
      "64": "res/icon64.png",
      "128": "res/icon128.png"
    }
  },
  "icons": {
    "16": "res/icon16.png",
    "32": "res/icon32.png",
    "64": "res/icon64.png",
    "128": "res/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2
}
